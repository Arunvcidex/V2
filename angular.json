{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "storyboard": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/storyboard",
      "sourceRoot": "projects/storyboard/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/storyboard",
            "index": "projects/storyboard/src/index.html",
            "main": "projects/storyboard/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/storyboard/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/storyboard/src/favicon.ico",
              "projects/storyboard/src/assets"
            ],
            "styles": [
              "node_modules/ngx-spinner/animations/ball-spin-clockwise.css",
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/intl-tel-input/build/css/intlTelInput.css",
              "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/froala-editor/css/froala_editor.pkgd.min.css",			
              "node_modules/froala-editor/css/froala_style.min.css",
              "projects/storyboard/src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/datatables.net-dt/js/dataTables.dataTables.js",
              "node_modules/bootstrap/dist/js/bootstrap.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "storyboard:build:production"
            },
            "development": {
              "browserTarget": "storyboard:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "storyboard:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/storyboard/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/storyboard/src/favicon.ico",
              "projects/storyboard/src/assets"
            ],
            "styles": [
              "projects/storyboard/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
